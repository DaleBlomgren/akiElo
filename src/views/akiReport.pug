html
 head
   p Winning Player
   select#winningPlayerTag(name='winningPlayerTag')
     each player in playerlist
       option=player 
   p Character Used
   select#winningPlayerCharacter(name='winningPlayerCharacter') 
     each character in ['Akuma', 'Alex', 'Ken', 'Chun li', 'Dudly', 'Yun', 'Yang', 'Sean', 'Oro', 'Makoto', 'Q', 'Necro', 'Ryu', 'Twelve', 'Hugo', 'Ibuki', 'Remy', 'Urien']
       option=character
   p Honorable Opponent
   select#losingPlayerTag(name='losingPlayerTag')
     each player in playerlist
       option=player
   p Character Used
   select#losingPlayerCharacter(name='losingPlayerCharacter') 
     each character in ['Akuma', 'Alex', 'Ken', 'Chun li', 'Dudly', 'Yun', 'Yang', 'Sean', 'Oro', 'Makoto', 'Q', 'Necro', 'Ryu', 'Twelve', 'Hugo', 'Ibuki', 'Remy', 'Urien']
       option=character
   script(type='application/javascript' src='/scripts/akiReport.js') 
   button(type="button", onclick="testDaJSON()") Submit match
